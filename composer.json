{
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.0",
        "phpunit/php-code-coverage": "^9.2"
    },
    "require": {
        "doctrine/orm": "^2.12.2",
        "doctrine/dbal": "^3.2",
        "doctrine/annotations": "^1.13",
        "symfony/cache": "^6.0",
        "symfony/http-foundation": "^6.0",
        "phpstan/phpstan": "^1.7",
        "league/booboo": "^2.1",
        "monolog/monolog": "^2.6"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/",
        "Doctrine\\Tests\\": "vendor/doctrine/tests"
      }
    }
}
